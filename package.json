{
  "name": "openchat",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prova": "npm run build:ts && node dist/index.js",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.test.ts\"",
    "start": "npm run prisma migrate deploy && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "format": "prettier . --write",
    "prisma": "prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/sensible": "^6.0.0",
    "@prisma/client": "^6.3.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.3.0"
  },
  "devDependencies": {
    "@prisma/migrate": "^6.3.1",
    "@testcontainers/postgresql": "^10.18.0",
    "@types/node": "^22.1.0",
    "axios": "^1.7.9",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^2.0.0",
    "prettier": "3.5.0",
    "prisma": "^6.3.0",
    "testcontainers": "^10.18.0",
    "ts-node": "^10.4.0",
    "typescript": "~5.6.3"
  }
}
